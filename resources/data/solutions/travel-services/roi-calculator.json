{
    "section": {
        "badge": "ROI Calculator",
        "title": "Calculate Your Travel Business",
        "highlighted": "AI Investment Returns",
        "description": "Discover exactly how much revenue Sawtic AI can generate for your UAE travel business. Our calculator uses real data from 250+ travel companies to provide accurate projections."
    },
    "calculator_inputs": [
        {
            "id": "monthly_calls",
            "label": "Monthly Incoming Calls",
            "type": "slider",
            "min": 100,
            "max": 10000,
            "default": 1500,
            "step": 100,
            "unit": "calls",
            "description": "Average number of calls your travel business receives monthly"
        },
        {
            "id": "current_answer_rate",
            "label": "Current Answer Rate",
            "type": "slider", 
            "min": 30,
            "max": 95,
            "default": 65,
            "step": 5,
            "unit": "%",
            "description": "Percentage of calls currently answered by your staff"
        },
        {
            "id": "average_booking_value",
            "label": "Average Booking Value",
            "type": "slider",
            "min": 500,
            "max": 15000,
            "default": 2500,
            "step": 250,
            "unit": "AED",
            "description": "Average value of travel bookings (hotels, tours, packages)"
        },
        {
            "id": "business_type",
            "label": "Business Type",
            "type": "select",
            "options": [
                {"value": "desert-tours", "label": "Desert Safaris & Tours", "multiplier": 1.2},
                {"value": "luxury-travel", "label": "Luxury Travel Packages", "multiplier": 1.8},
                {"value": "corporate-travel", "label": "Corporate Travel Management", "multiplier": 1.4},
                {"value": "cultural-tours", "label": "Cultural & Heritage Tours", "multiplier": 1.1},
                {"value": "hotel-booking", "label": "Hotel Reservations", "multiplier": 1.0},
                {"value": "general-agency", "label": "General Travel Agency", "multiplier": 1.3}
            ],
            "default": "general-agency",
            "description": "Your primary travel business focus area"
        },
        {
            "id": "peak_season_factor",
            "label": "Peak Season Intensity",
            "type": "slider",
            "min": 1.0,
            "max": 3.0,
            "default": 2.2,
            "step": 0.2,
            "unit": "x",
            "description": "How much busier you get during UAE peak tourism season"
        }
    ],
    "calculation_formulas": {
        "missed_calls_monthly": "monthly_calls * (1 - current_answer_rate/100)",
        "ai_capture_rate": 0.96,
        "recovered_bookings": "missed_calls_monthly * ai_capture_rate * 0.35",
        "additional_revenue": "recovered_bookings * average_booking_value * business_type_multiplier",
        "peak_season_boost": "additional_revenue * (peak_season_factor - 1) * 4/12",
        "total_monthly_increase": "additional_revenue + peak_season_boost",
        "annual_revenue_increase": "total_monthly_increase * 12",
        "implementation_cost": 35000,
        "monthly_cost": 2800,
        "annual_operating_cost": "monthly_cost * 12",
        "net_annual_profit": "annual_revenue_increase - annual_operating_cost",
        "roi_percentage": "(net_annual_profit / implementation_cost) * 100",
        "payback_months": "implementation_cost / (total_monthly_increase - monthly_cost)"
    },
    "results_display": {
        "primary_metrics": [
            {
                "label": "Additional Monthly Revenue",
                "key": "total_monthly_increase",
                "format": "AED {:,}",
                "icon": "uil-money-bill",
                "color": "success"
            },
            {
                "label": "Annual Revenue Increase", 
                "key": "annual_revenue_increase",
                "format": "AED {:,}",
                "icon": "uil-chart-growth",
                "color": "primary"
            },
            {
                "label": "ROI Percentage",
                "key": "roi_percentage", 
                "format": "{:.0f}%",
                "icon": "uil-percentage",
                "color": "success"
            },
            {
                "label": "Payback Period",
                "key": "payback_months",
                "format": "{:.1f} months", 
                "icon": "uil-clock",
                "color": "info"
            }
        ],
        "detailed_breakdown": [
            {
                "category": "Call Volume Analysis",
                "metrics": [
                    {"label": "Monthly Calls", "key": "monthly_calls", "format": "{:,}"},
                    {"label": "Currently Missed", "key": "missed_calls_monthly", "format": "{:,}"},
                    {"label": "AI Will Capture", "key": "recovered_bookings", "format": "{:,}"}
                ]
            },
            {
                "category": "Revenue Impact",
                "metrics": [
                    {"label": "Base Monthly Increase", "key": "additional_revenue", "format": "AED {:,}"},
                    {"label": "Peak Season Bonus", "key": "peak_season_boost", "format": "AED {:,}"},
                    {"label": "Total Annual Increase", "key": "annual_revenue_increase", "format": "AED {:,}"}
                ]
            },
            {
                "category": "Investment Analysis",
                "metrics": [
                    {"label": "Implementation Cost", "key": "implementation_cost", "format": "AED {:,}"},
                    {"label": "Annual Operating Cost", "key": "annual_operating_cost", "format": "AED {:,}"},
                    {"label": "Net Annual Profit", "key": "net_annual_profit", "format": "AED {:,}"}
                ]
            }
        ]
    },
    "assumptions": [
