    "benefits_calculation": [
        {
            "category": "Revenue Recovery",
            "description": "Revenue from previously missed calls",
            "formula": "monthly_calls * (missed_call_rate/100) * avg_job_value * 0.80",
            "impact_type": "positive"
        },
        {
            "category": "Operational Efficiency", 
            "description": "Reduced staffing and administrative costs",
            "formula": "(staff_count - 1) * 3500",
            "impact_type": "positive"
        },
        {
            "category": "24/7 Availability Premium",
            "description": "Additional revenue from after-hours bookings",
            "formula": "monthly_calls * 0.30 * avg_job_value",
            "impact_type": "positive"
        },
        {
            "category": "AI Solution Investment",
            "description": "Monthly cost of AI implementation",
            "formula": "799 + (monthly_calls * 2.50)",
            "impact_type": "negative"
        }
